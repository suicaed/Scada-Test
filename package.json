    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^1.3.5",
    "react-hot-loader": "^4.13.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.6",
