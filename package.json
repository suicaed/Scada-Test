    "url": "https://github.com/suicaed/Scada-Test/issues"
  },
  "homepage": "https://github.com/suicaed/Scada-Test#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3,
          "targets": "> 0.25%, not dead"
        }
      ],
      "@babel/preset-react"
    ],
